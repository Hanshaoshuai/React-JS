{
    "name": "ali-conf-energy",
    "options": {
        "type": "uxcore",
        "template": "https://github.com/nowa-webpack/template-uxcore/archive/v4.zip",
        "npm": "https://registry.npm.taobao.org",
        "vars": {
            "locale": "zh",
            "__LOCAL__": true
        },
        "buildvars": {
            "__LOCAL__": [false],
            "locale": ["zh"]
        },
        "pages": false,
        "externals": {},
        "alias": {
            "variables": "app/variables.js",
            "app": "app",
            "components": "components",
            "models": "models",
            "services": "services",
            "pages": "pages",
            "i18n": "i18n"
        }
    },
    "assets": {
        "type": "command",
        "command": {
            "cmd": [
                "tnpm ii",
                "tnpm run cloud"
            ]
        }
    }
}
